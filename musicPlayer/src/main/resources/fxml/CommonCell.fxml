<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
    <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" fx:id="icon"/>
    <Label layoutX="40.0" prefHeight="20.0" prefWidth="220.0" text="Label" fx:id="title">
        <font>
            <Font size="13.0"/>
        </font>
    </Label>
    <Label layoutX="40.0" layoutY="20.0" prefHeight="20.0" prefWidth="220.0" text="Label" fx:id="content"/>
    <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="260.0" pickOnBounds="true" preserveRatio="true"
               fx:id="operation"/>
</fx:root>
